                    </a>
                    <a href="/homepage/{{this._id}}" class="like-button"><i class="fa fa-heart-o"></i></a>
                    <div class="product-info">
                        <a href="/productpage/{{this._id}}" class="title">{{this.productname}}</a>
                        <div class="rating">
                            <span class="fa fa-star checked"></span>
                            <span class="fa fa-star checked"></span>
                            <span class="fa fa-star checked"></span>
                            <span class="fa fa-star"></span>
                            <span class="fa fa-star"></span>
                            {{this.rating}}
                        </div>
                        <div class="price">
